mirrors:
{% for mirror in mirrored_registries %}
  {{ mirror.name }}:
    endpoint:
      - "{{ mirror.endpoint }}"
{% endfor %}
{% if registry_configs is defined -%}
configs:
{% for registry in registry_configs %}
  "{{ registry.url }}":
    {% if registry.auth is defined -%}
    auth:
      username: {{ registry.auth.username }}
      password: {{ registry.auth.password }}
    {%- endif %}
{% endfor %}
{% endif %}
